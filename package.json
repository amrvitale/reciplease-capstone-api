    "logger": "0.0.1",
    "morgan": "^1.10.0",
    "pg": "7.17.1",
    "postgrator": "^4.0.1",
    "winston": "^3.3.3",
    "xss": "^1.0.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
